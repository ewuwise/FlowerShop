  }
}
```

### **2. Create App.js**
```javascript
import React from 'react';
import { NavigationContainer } from '@react-navigation/native';
import { createStackNavigator } from '@react-navigation/stack';
import { CartProvider } from './context/CartContext';
import FlowerLandingPage from './components/FlowerLandingPage';
import ProductDetailScreen from './components/ProductDetailScreen';
import CartScreen from './components/CartScreen';

const Stack = createStackNavigator();

export default function App() {
  return (
    <CartProvider>
      <NavigationContainer>
        <Stack.Navigator 
          initialRouteName="FlowerLandingPage"
          screenOptions={{
            headerStyle: {
              backgroundColor: '#2d5a3d',
            },
            headerTintColor: '#fff',
            headerTitleStyle: {
              fontWeight: 'bold',
            },
          }}
        >
          <Stack.Screen 
            name="FlowerLandingPage" 
            component={FlowerLandingPage}
            options={{ headerShown: false }}
          />
          <Stack.Screen 
            name="ProductDetail" 
            component={ProductDetailScreen}
            options={{ title: 'Product Details' }}
          />
          <Stack.Screen 
            name="Cart" 
            component={CartScreen}
            options={{ title: 'Shopping Cart' }}
          />
        </Stack.Navigator>
      </NavigationContainer>
    </CartProvider>
  );
}
```

### **3. Create context/CartContext.js**
```javascript
import React, { createContext, useState, useContext } from 'react';

const CartContext = createContext();

export const useCart = () => {
  const context = useContext(CartContext);
  if (!context) {
    throw new Error('useCart must be used within a CartProvider');
  }
  return context;
};

export const CartProvider = ({ children }) => {
  const [cartItems, setCartItems] = useState([]);

  const addToCart = (product) => {
    setCartItems(prevItems => {
      const existingItem = prevItems.find(item => item.id === product.id);
      if (existingItem) {
        return prevItems.map(item =>
          item.id === product.id
            ? { ...item, quantity: item.quantity + 1 }
            : item
        );
      }
      return [...prevItems, { ...product, quantity: 1 }];
    });
  };

  const removeFromCart = (productId) => {
    setCartItems(prevItems => prevItems.filter(item => item.id !== productId));
  };

  const updateQuantity = (productId, newQuantity) => {
    if (newQuantity < 1) {
      removeFromCart(productId);
      return;
    }
    setCartItems(prevItems =>
      prevItems.map(item =>
        item.id === productId ? { ...item, quantity: newQuantity } : item
      )
    );
  };

  const clearCart = () => {
    setCartItems([]);
  };

  const getTotalPrice = () => {
    return cartItems.reduce((total, item) => total + (item.price * item.quantity), 0);
  };

  const getTotalItems = () => {
    return cartItems.reduce((total, item) => total + item.quantity, 0);
  };

  const value = {
    cartItems,
    addToCart,
    removeFromCart,
    updateQuantity,
    clearCart,
    getTotalPrice,
    getTotalItems,
  };

  return (
    <CartContext.Provider value={value}>
      {children}
    </CartContext.Provider>
  );
};
```

### **4. Create components/FlowerLandingPage.js**
```javascript
import React, { useRef, useEffect, useState } from 'react';
import { 
  View, 
  Text, 
  Image, 
  TextInput, 
  ScrollView, 
  TouchableOpacity, 
  Animated,
  StatusBar
} from 'react-native';
import { useNavigation } from '@react-navigation/native';
import { useCart } from '../context/CartContext';
import { styles } from './styles';

const FlowerLandingPage = () => {
  const navigation = useNavigation();
  const { addToCart, cartItems } = useCart();
  const [searchQuery, setSearchQuery] = useState('');
  
  const fadeAnim = useRef(new Animated.Value(0)).current;
  const slideAnim = useRef(new Animated.Value(50)).current;

  useEffect(() => {
    Animated.parallel([
      Animated.timing(fadeAnim, {
        toValue: 1,
        duration: 1000,
        useNativeDriver: true,
      }),
      Animated.timing(slideAnim, {
        toValue: 0,
        duration: 800,
        useNativeDriver: true,
      }),
    ]).start();
  }, []);

  const flowerSections = [
    {
      title: "Featured Flowers",
      flowers: [
        {
          id: 1,
          name: "White Base",
          price: 45,
          image: "https://images.unsplash.com/photo-1686125616977-34f6d5979eb1?q=80&w=1856",
          description: "Beautiful white flowers arranged elegantly"
        },
        {
          id: 2,
          name: "Red Rose",
          price: 35,
          image: "https://images.unsplash.com/photo-1581264692636-3cf6f29655c2?q=80&w=1887",
          description: "Classic red roses symbolizing love"
        },
        {
          id: 3,
          name: "Lily",
          price: 40,
          image: "https://images.unsplash.com/photo-1579847621515-b40fcc20831e?q=80&w=1887",
          description: "Elegant lilies for any occasion"
        },
        {
          id: 4,
          name: "Tulip",
          price: 38,
          image: "https://images.unsplash.com/photo-1487435636644-3ad040f0195b?q=80&w=2070",
          description: "Colorful tulips to brighten your day"
        }
      ]
    },
    {
      title: "Mother's Day Special",
      flowers: [
        {
          id: 5,
          name: "Pink Carnations",
          price: 55,
          image: "https://images.unsplash.com/photo-1519378058457-4c29a0a2efac?q=80&w=1964",
          description: "Traditional flowers for Mother's Day"
        },
        {
          id: 6,
          name: "Mixed Bouquet",
          price: 65,
          image: "https://images.unsplash.com/photo-1563241527-3004b7be0ffd?q=80&w=1887",
          description: "Special mixed bouquet for mom"
        },
        {
          id: 7,
          name: "Sunflowers",
          price: 48,
          image: "https://images.unsplash.com/photo-1490750967868-88aa4486c946?q=80&w=2070",
          description: "Bright sunflowers to bring joy"
        }
      ]
    },
    {
      title: "Anniversary Celebrations",
      flowers: [
        {
          id: 8,
          name: "Red Roses",
          price: 75,
          image: "https://images.unsplash.com/photo-1563241527-3004b7be0ffd?q=80&w=1887",
          description: "Luxurious red roses for celebrations"
        },
        {
          id: 9,
          name: "Romantic Mix",
          price: 85,
          image: "https://images.unsplash.com/photo-1578300751624-ebc7193f784a?q=80&w=1887",
          description: "Romantic mix for special moments"
        },
        {
          id: 10,
          name: "Luxury Bouquet",
          price: 95,
          image: "https://images.unsplash.com/photo-1496062031455-6e52e2c2fd98?q=80&w=2059",
          description: "Extravagant bouquet for milestones"
        }
      ]
    }
  ];

  const handleCardPress = (flower) => {
    navigation.navigate('ProductDetail', { product: flower });
  };

  const handleAddToCart = (flower) => {
    addToCart(flower);
  };

  const filteredSections = flowerSections.map(section => ({
    ...section,
    flowers: section.flowers.filter(flower => 
      flower.name.toLowerCase().includes(searchQuery.toLowerCase())
    )
  })).filter(section => section.flowers.length > 0);

  return (
    <View style={styles.container}>
      <StatusBar backgroundColor="#2d5a3d" barStyle="light-content" />
      
      {/* Header with Cart */}
      <View style={styles.header}>
        <View>
          <Text style={styles.title}>Blooming Flowers</Text>
          <Text style={styles.subtitle}>
            Beautiful flowers for every occasion
          </Text>
        </View>
        <TouchableOpacity 
          style={styles.cartIcon}
          onPress={() => navigation.navigate('Cart')}
        >
          <Text style={styles.cartCount}>{cartItems.length}</Text>
          <Text style={styles.cartIconText}>üõí</Text>
        </TouchableOpacity>
      </View>

      <ScrollView 
        style={styles.scrollView}
        showsVerticalScrollIndicator={false}
      >
        {/* Hero Image */}
        <Animated.View 
          style={[
            styles.heroContainer,
            {
              opacity: fadeAnim,
              transform: [{ translateY: slideAnim }]
            }
          ]}
        >
          <Image
            source={{ uri: "https://images.unsplash.com/photo-1463043254199-7a3efd782ad1?q=80&w=2069" }}
            style={styles.heroImage}
          />
        </Animated.View>

        {/* Search Bar */}
        <Animated.View 
          style={[
            styles.searchContainer,
            {
              opacity: fadeAnim,
              transform: [{ translateY: slideAnim }]
            }
          ]}
        >
          <TextInput
            placeholder="Search for flowers..."
            style={styles.searchInput}
            placeholderTextColor="#888"
            value={searchQuery}
            onChangeText={setSearchQuery}
          />
        </Animated.View>

        {/* Flower Sections */}
        {filteredSections.map((section, index) => (
          <FlowerSection 
            key={section.title}
            title={section.title}
            flowers={section.flowers}
            onCardPress={handleCardPress}
            onAddToCart={handleAddToCart}
            animationDelay={index * 200}
          />
        ))}
      </ScrollView>
    </View>
  );
};

const FlowerSection = ({ title, flowers, onCardPress, onAddToCart, animationDelay }) => {
  const sectionFadeAnim = useRef(new Animated.Value(0)).current;
  const sectionSlideAnim = useRef(new Animated.Value(30)).current;

  useEffect(() => {
    const timer = setTimeout(() => {
      Animated.parallel([
        Animated.timing(sectionFadeAnim, {
          toValue: 1,
          duration: 600,
          useNativeDriver: true,
        }),
        Animated.timing(sectionSlideAnim, {
          toValue: 0,
          duration: 500,
          useNativeDriver: true,
        }),
      ]).start();
    }, animationDelay);

    return () => clearTimeout(timer);
  }, []);

  return (
    <Animated.View 
      style={[
        styles.section,
        {
          opacity: sectionFadeAnim,
          transform: [{ translateY: sectionSlideAnim }]
        }
      ]}
    >
      <View style={styles.sectionHeader}>
        <Text style={styles.sectionTitle}>{title}</Text>
        <TouchableOpacity>
          <Text style={styles.viewAllText}>View All</Text>
        </TouchableOpacity>
      </View>

      <ScrollView 
        horizontal 
        showsHorizontalScrollIndicator={false}
        style={styles.horizontalScroll}
      >
        {flowers.map((flower) => (
          <TouchableOpacity 
            key={flower.id} 
            style={styles.card}
            onPress={() => onCardPress(flower)}
          >
            <Image source={{ uri: flower.image }} style={styles.cardImage} />
            <View style={styles.cardContent}>
              <Text style={styles.cardLabel}>{flower.name}</Text>
              <Text style={styles.cardPrice}>${flower.price}</Text>
              <TouchableOpacity 
                style={styles.addToCartBtn}
                onPress={() => onAddToCart(flower)}
              >
                <Text style={styles.addToCartText}>Add to Cart</Text>
              </TouchableOpacity>
            </View>
          </TouchableOpacity>
        ))}
      </ScrollView>
    </Animated.View>
  );
};

export default FlowerLandingPage;
```

